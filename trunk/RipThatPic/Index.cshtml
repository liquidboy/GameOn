@using System.Web.Optimization;
@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" ng-app="bootstrapApp">
<head>
    <title>Rip that Pic!</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="CACHE-CONTROL" content="NO-CACHE">
    
    @Styles.Render("~/default/css-libraries")
</head>
<body>


    <div>
        <div ng-animate ng-view="">
        </div>
    </div>



    <!-- NOTIFICATIONS -->
    <div class="notifications-right">
        <ul id="alerts"></ul>
    </div>




    @Scripts.Render("~/default/thirdparty-js-libraries")




    @*@Scripts.Render("~/default/thirdparty-js-libraries")*@
    <script src="/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="signalr/hubs"></script>




    <!-- BOOTSTRAP APP -->
    <script src="/angularApp/BootstrapApp.js"></script>


    <!-- SERVICES -->
    @Scripts.Render("~/default/services-js-libraries")
    

    <!-- CONSTANTS -->
    <script src="/angularApp/Constants.js"></script>


    <!-- CONTROLLERS -->
    <script src="/angularApp/controllers/home.js"></script>
    <script src="/angularApp/controllers/login.js"></script>
    <script src="/angularApp/controllers/splash.js"></script>
    <script src="/angularApp/controllers/explorer.js"></script>
    <script src="/angularApp/controllers/page.js"></script>
    @Scripts.Render("~/config/controllers-js-libraries")
    

    <!-- DIRECTIVES -->
    @Scripts.Render("~/default/directives-js-libraries")
    

</body>
</html>


